{
    "version": 2,
    "scope": "nodejs-website",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/",
            "dest": "/server.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/(.css)",
            "dest": "/server.js",
            "methods": [
                "GET"
            ],
            "continue": true
        },
        {
            "src": "/img/(.*)",
            "dest": "/server.js",
            "methods": [
                "GET"
            ],
            "continue": true
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/server.js",
            "methods": [
                "GET"
            ]
        }
    ]
}