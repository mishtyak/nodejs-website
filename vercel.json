{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "public": true,
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/dist/(.*)",
            "dest": "/server.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/favicon.png",
            "dest": "/server.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/manifest.webmanifest",
            "dest": "/server.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/img/(.*)",
            "dest": "/server.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/",
            "dest": "/server.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/fm",
            "dest": "/server.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/(.*)",
            "status": 404,
            "dest": "/server.js",
            "methods": [
                "GET"
            ]
        }
    ]
}